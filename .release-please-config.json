{
  "pull-request-header": "Release",
  "changelog-path": "CHANGELOG.md",
  "bump-minor-pre-major": true,
  "bump-patch-for-minor-pre-major": true,
  "draft": false,
  "include-component-in-tag": true,
  "include-v-in-tag": true,
  "pull-request-title-pattern": "chore(release): ${component}-v${version}",
  "prerelease": true,
  "release-type": "rust",
  "initial-version": "0.0.1",
  "separate-pull-requests": true,
  "draft-pull-request": true,
  "pull-request-footer": "This release was generated with [Release Please](https://github.com/googleapis/release-please).",
  "packages": {
    ".": {
      "pull-request-title-pattern": "chore(release): v${version}",
      "include-component-in-tag": false,
      "extra-files": ["pyproject.toml"],
      "exclude-paths": ["crates"]
    },
    "crates/squirrel": {
      "release-as": "0.0.1"
    }
  },
  "plugins": [
    {
      "type": "sentence-case"
    }
  ],
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json"
}
