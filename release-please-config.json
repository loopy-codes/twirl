{
  "packages": {
    ".": {
      "pull-request-header": "Release",
      "changelog-path": "CHANGELOG.md",
      "bump-minor-pre-major": true,
      "bump-patch-for-minor-pre-major": true,
      "draft": false,
      "include-component-in-tag": false,
      "include-v-in-tag": true,
      "pull-request-title-pattern": "release: v${version}",
      "prerelease": true,
      "release-type": "rust",
      "initial-version": "0.0.1",
      "changelog-sections": [
        { "type": "feat", "section": "Features" },
        { "type": "feature", "section": "Features" },
        { "type": "fix", "section": "Bug Fixes" },
        { "type": "bug", "section": "Bug Fixes" },
        { "type": "doc", "section": "Documentation" },
        { "type": "docs", "section": "Documentation" },
        { "type": "test", "section": "Testing" },
        { "type": "tests", "section": "Testing" },
        { "type": "chore", "section": "Project Configuration" },
        { "type": "ci", "section": "Continuous Integration" },
        { "type": "req", "section": "Requirements Changes" },
        { "type": "refactor", "section": "Code Refactoring" }
      ]
    }
  },
  "plugins": [
    {
      "type": "sentence-case"
    }
  ],
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json"
}
